pipeline{
		agent any
		stages{
				stage('Submit Stack'){
					steps{
					sh "cd /var/jenkins_home/test_aws/bin/"
					sh "pwd"
					sh "ls -b"
					sh "/var/jenkins_home/test_aws/bin/aws cloudformation create-stack --stack-name ecinstancetest --template-body file://cf_script.json --tags Key=Name,Value=testserver Key=costid,Value=123 --region 'us-west-2'"
					}
				}
		}
}
